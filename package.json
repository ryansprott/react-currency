{
  "name": "react-currency",
  "version": "1.0.0",
  "description": "Simple React app for currency conversion.",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build-html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build-html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryansprott/react-currency.git"
  },
  "author": "Ryan Sprott",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryansprott/react-currency/issues"
  },
  "homepage": "https://github.com/ryansprott/react-currency#readme",
  "dependencies": {
    "block-ui": "^2.70.1",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.3",
    "eslint": "^4.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.12.3",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "jquery": "^3.4.1",
    "jquery-ui-dist": "^1.12.1",
    "npm-run-all": "^4.1.5",
    "open": "^6.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "url-loader": "^1.1.2",
    "webpack-hot-middleware": "^2.18.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "path": "^0.12.7",
    "style-loader": "^0.18.2",
    "webpack": "^4.28.3",
    "webpack-dev-server": "^3.1.11"
  }
}
